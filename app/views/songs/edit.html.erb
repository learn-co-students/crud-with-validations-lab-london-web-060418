<%= form_for @song do |f| %>
  <%= f.label "Song title" %><br>
  <%= f.text_field :title %><br>
  <%= f.label "Artist" %><br>
  <%= f.text_field :artist_name %><br>
  <%= f.label "Released?" %><br>
  <%= f.check_box :released%><br>
  <%= f.label "Release year" %><br>
  <%= f.text_field :release_year%><br>
  <%= f.label "Genre" %><br>
  <%= f.text_field :genre%><br>

  <%= f.submit %>
<% end %>
